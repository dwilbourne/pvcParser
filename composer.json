{
	"name": "pvc/parser",
	"description": "pvc parser library",
	"type": "project",
	"authors": [
		{
			"name": "Doug Wilbourne",
			"email": "dougwilbourne@gmail.com"
		}
	],
	"license": "MIT",
	"config": {
		"php": "8.1"
	},
	"autoload": {
		"psr-4": {
			"pvc\\parser\\": "src/",
			"pvcTests\\parser\\": "tests/",
			"pvcExamples\\parser\\": "examples/"
		}
	},
	"minimum-stability": "dev",
	"require": {
		"ext-intl": "*",
		"ext-json": "*",
		"ext-iconv": "*",
		"pvc/interfaces": "dev-removedefs",
		"pvc/err": "*"
	},
	"require-dev": {
		"pvc/http": "*",
		"phpunit/phpunit": "^9",
		"squizlabs/php_codesniffer": "*",
		"friendsofphp/php-cs-fixer": "*",
		"phpstan/phpstan": "*"
	}
}
