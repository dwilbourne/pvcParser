{
	"name": "pvc/parser",
	"description": "pvc parser library",
	"type": "project",
	"authors": [
		{
			"name": "Doug Wilbourne",
			"email": "dougwilbourne@gmail.com"
		}
	],
	"license": "MIT",
	"config": {
		"php": "7.4.3"
	},
	"autoload": {
		"psr-4": {
			"pvc\\": "src/",
			"tests\\": "tests/"
		}
	},
	"require": {
		"ext-intl": "*",
		"ext-json": "*",
		"ext-iconv": "*",
		"ext-gettext": "*",
		"ext-dom": "*",
		"ext-fileinfo": "*",
		"ext-libxml": "*",
		"ext-simplexml": "*",
		"nikic/php-parser": "^4.4",
		"pvc/interfaces": "@dev",
		"pvc/msg": "@dev",
		"pvc/err": "@dev",
		"pvc/regex": "@dev",
		"pvc/intl": "@dev",
		"pvc/utils": "@dev",
		"pvc/frmtr": "@dev",
		"pvc/struct": "@dev",
		"pvc/http": "@dev"
	},
	"require-dev": {
		"phpunit/phpunit": "^8",
		"mockery/mockery": "dev-master",
		"phpspec/prophecy": "~1.0",
		"squizlabs/php_codesniffer": "^3.5",
		"friendsofphp/php-cs-fixer": "3.0.x-dev",
		"guzzlehttp/guzzle": "^6.5",
		"pvc/testing-traits": "dev-master",
		"pvc/storage": "dev-master",
		"rexxars/html-validator": "^2.2.0",
		"phpstan/phpstan": "^0.12.25"
	}
}
